{
  "manifest_version": 3,
  "name": "CyberSaarthi",
  "version": "2.0.0",
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["activeTab", "storage", "alarms"],
  "host_permissions": ["http://127.0.0.1:8000/*", "http://localhost:8000/*"]
}
